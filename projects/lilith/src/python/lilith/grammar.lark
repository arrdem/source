%import common.WORD
%import common.NUMBER
%import common.WS
%ignore WS

STRING: /""".*?"""/ | /".*?"/

atom: NUMBER | WORD | STRING

expr: atom

args: expr ("," args)?

kwargs: expr ":" expr ("," kwargs)?

_args_kwargs: args ";" kwargs
_args: args
_kwargs: kwargs
arguments: _args_kwargs | _args | _kwargs

header: "!" WORD "[" arguments? "]"
